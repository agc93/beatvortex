    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/semver": "^7.1.0",
    "axios": "^0.19.2",
    "bluebird": "^3.5.5",
    "electron": "4.2.4",
    "i18next": "^17.0.4",
    "react": "^16.8.6",
    "react-async": "^10.0.1",
    "react-bootstrap": "^0.32.4",
